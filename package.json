{
  "name": "synapi-client",
  "version": "0.0.2",
  "description": "A wrapper for fetch to make things more easier",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && npm run test:built",
    "test:built": "mocha --compilers js:babel-register",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel src --out-dir lib",
    "build:watch": "npm run clean && babel src --out-dir lib --watch",
    "lint": "eslint src",
    "prepublish": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synapsestudios/synapi-client.git"
  },
  "author": "Synapse Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/synapsestudios/synapi-client/issues"
  },
  "homepage": "https://github.com/synapsestudios/synapi-client#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "estraverse-fb": "^1.3.1",
    "form-data": "^1.0.0-rc4",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "urlsearchparams": "^0.1.1",
    "whatwg-fetch": "^0.11.0"
  },
  "dependencies": {
    "eventemitter2": "^1.0.0",
    "merge": "^1.2.0"
  }
}
